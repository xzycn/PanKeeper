{
  "manifest_version": 2,
  "name": "PanKeeper",
  "description": "Keep the resource you have accessed ever accessible",
  "version": "1.0.0",


  "permissions": [
    "webRequest", "webRequestBlocking","https://pan.baidu.com/","tabs"
  ],
  "background": {
    "scripts": [
      "js/pouchdb.min.js",
      "js/db.js",
      "js/background.js"
    ]
  },
  "content_scripts":[
     {
      "matches": ["https://pan.baidu.com/*"],
      "js": ["js/content.js"]
    }
  ]
}
